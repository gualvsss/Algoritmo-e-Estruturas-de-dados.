
#include <stdio.h>
#include <locale.h>

 int main(){
 	 
 	 setlocale(LC_ALL, "Portuguese");
 	 int valor;
 	 printf("------- Início de Programa ------\n");
 	 printf("Entre com um valor numérico inteiro:\n");
 	 
 	 scanf("%d", &valor);
 	 printf("\nValor entrado: %d\n\n", valor);
 	 
 	 if (valor > 10)
	     printf("Valor entrado maior que 10\n\n");
	else
		 printf("Valor entrado não é maior que 10 !\n\n");
		 
	printf("------- Fim de Programa --------");
	
	return 0;
		  
 }
